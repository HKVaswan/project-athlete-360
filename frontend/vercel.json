{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://project-athlete-360.onrender.com/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ]
}