{
  "name": "project-athlete-360-backend",
  "version": "1.0.0",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --no-notify src/server.ts",
    "build": "npm run clean && tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "jest --passWithNoTests",
    "clean": "rimraf dist",
    "check": "npm run lint && npm run test",
    "prepare": "husky install || true",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate deploy",
    "postinstall": "npx prisma generate"
  },
  "engines": {
    "node": ">=20.0.0 <26.0.0"
  },
  "dependencies": {
    "@opentelemetry/api": "1.8.0",
    "@opentelemetry/sdk-node": "0.51.1",
    "@prisma/client": "6.19.0",
    "@sentry/node": "8.3.0",
    "bcrypt": "5.1.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.21.1",
    "express-rate-limit": "7.2.0",
    "express-xss-sanitizer": "1.2.1",
    "helmet": "7.1.0",
    "hpp": "0.2.3",
    "ioredis": "5.4.1",
    "joi": "17.9.2",
    "jsonwebtoken": "9.0.0",
    "morgan": "1.10.0",
    "prom-client": "15.1.0",
    "tslib": "2.6.0",
    "uuid": "9.0.0",
    "winston": "3.13.0",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "9.0.2",
    "@types/node": "22.2.0",
    "@types/morgan": "1.9.4",
    "eslint": "8.56.0",
    "husky": "9.1.3",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "prisma": "6.19.0",
    "rimraf": "5.0.7",
    "ts-jest": "29.1.0",
    "ts-node-dev": "2.0.0",
    "typescript": "5.5.4"
  },
  "license": "MIT"
}