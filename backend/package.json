{
 "name": "project-athlete-360-backend",
 "version": "0.1.0",
 "private": true,
 "description": "Backend for Project Athlete 360 (TypeScript + Prisma)",
 "main": "dist/server.js",
 "engines": {
 "node": ">=20.0.0 <26.0.0",
 "npm": ">=9.0.0"
 },
 "scripts": {
 "clean": "rimraf dist",
 "dev": "ts-node-dev --respawn --transpile-only --no-notify src/server.ts",
 "build": "npm run clean && tsc -p tsconfig.json",
 "start": "node dist/server.js",
 "start:prod": "cross-env NODE_ENV=production node dist/server.js",
 "lint": "eslint \"src//.{ts,js}\" --cache",
 "lint:fix": "npm run lint -- --fix",
 "format": "prettier --write \"/.{ts,js,json,md}\"",
 "typecheck": "tsc -p tsconfig.json --noEmit",
 "test": "jest --passWithNoTests --runInBand",
 "test:watch": "jest --watch",
 "check": "npm run lint && npm run typecheck && npm run test",
 "prepare": "husky install",
 "prisma:generate": "prisma generate",
 "prisma:migrate": "prisma migrate deploy",
 "prisma:dev": "prisma migrate dev",
 "prisma:studio": "prisma studio",
 "postinstall": "prisma generate && echo \"Prisma client generated successfully\""
 },
 "dependencies": {
 "@prisma/client": "^4.15.0",
 "dotenv": "^16.0.3"
 },
 "devDependencies": {
 "prisma": "^4.15.0",
 "typescript": "^5.1.6",
 "ts-node-dev": "^2.0.0",
 "rimraf": "^5.0.0",
 "eslint": "^8.46.0",
 "eslint-config-prettier": "^8.8.0",
 "prettier": "^2.8.8",
 "jest": "^29.6.0",
 "ts-jest": "^29.1.1",
 "@types/jest": "^29.5.3",
 "@types/node": "^20.11.0",
 "husky": "^8.0.3",
 "lint-staged": "^14.3.0",
 "cross-env": "^7.0.3"
 },
 "lint-staged": {
 ".{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
 ".{json,md}": [
      "prettier --write"
    ]
 },
 "repository": {
 "type": "git",
 "url": "https://github.com/HKVaswan/project-athlete-360.git"
 },
 "bugs": {
 "url": "https://github.com/HKVaswan/project-athlete-360/issues"
 },
 "homepage": "https://github.com/HKVaswan/project-athlete-360#readme",
 "license": "UNLICENSED"
}